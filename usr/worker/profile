load std

# Set up expected directories and files
mkdir -p /keydb
mkdir -p $home/keyring

touch /keydb/keys

# Create common mount points
mkdir -p /mnt
mkdir -p /n

mount -a {mntgen} /mnt
mount -a {mntgen} /n

# Set up network stack
ndb/cs
ndb/dns

# Run file and cpu servers if key is present
and {ftest -f $home^/keyring/default} {svc/net}
